{
    "name": "reactor/transit",
    "description": "Easy file uploading and downloading for Laravel 5",
    "license": "MIT",
    "keywords": [
        "reactor",
        "transit",
        "upload",
        "download",
        "laravel",
        "laravel5"
    ],
    "version": "5.6.0",
    "authors": [
        {
            "name": "Matrix Infoline",
            "email": "infolinematrix@gmail.com",
            "homepage": "http://scriptbazar.com",
            "role": "Developer / Designer"
        }
    ],
	"require": {
        "php": ">=7.1.3"
	},
	"autoload": {
		"psr-4": {
            "Reactor\\Transit\\": "src/"
		},
        "files": [
            "src/Support/helpers.php"
        ]
	},
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "suggest": {
        "laravel/framework": "This package requires Laravel 5.",
        "kenarkose/ownable": "Allows ownership for Eloquent models on Laravel 5."
    }
}
